{
  "name": "go-stripe",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./assets/css/input.css -o ./static/css/output.css",
    "watch:css": "tailwindcss -i ./assets/css/input.css -o ./static/css/output.css --watch",
    "build:js": "vite build",
    "watch:js": "vite build --watch",
    "build": "npm-run-all build:css build:js",
    "watch": "npm-run-all --parallel watch:css watch:js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@stripe/stripe-js": "^8.6.0",
    "@tailwindcss/cli": "^4.1.13",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^25.0.3",
    "glob": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}
